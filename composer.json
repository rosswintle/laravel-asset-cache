{
    "name": "rosswintle/laravel-javascript-package-manager",
    "description": "Package manager for inclusion of non-hierarchical JavaScript packages in Laravel projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Wintle",
            "email": "ross@oikos.org.uk"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4":  {
            "RossWintle\\LaravelJPM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4":  {
            "RossWintle\\LaravelJPM\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "~4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RossWintle\\LaravelJPM\\LaravelJPMServiceProvider"
            ],
            "aliases": {
                "LaravelJPM": "RossWintle\\LaravelJPM\\Facades\\LaravelJPM"
            }
        }
    }
}
